{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "https://your-backend-domain.com/api/v1/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
